[user]
	email = icken.jannis@gmail.com
	name = Jannis Icken
[credential]
	helper = store
[init]
	defaultBranch = main
[core]
	editor = nvim
[column]
	ui = auto
[branch]
	sort = -committerdate
[tag]
	sort = version:refname
[diff]
	algorithm = histogram
	colorMoved = plain
	renames = true
	mnemonicPrefix = true
[push]
	autoSetupRemote = true
[fetch]
	prune = true
	pruneTags = true
	all = true
[commit]
	verbose = true
[pull]
	rebase = false
